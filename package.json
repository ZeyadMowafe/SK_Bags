{
  "name": "handmade-bags-project",
  "version": "1.0.0",
  "description": "Handmade Bags E-commerce Full Stack Project",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && python run.py",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd frontend && npm start"
  },
  "keywords": [
    "ecommerce",
    "handmade",
    "bags",
    "react",
    "fastapi",
    "supabase"
  ],
  "author": "Handmade Bags Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
